<h1>Doctor</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Apellido</th>
            <td>{{ entity.Apellido }}</td>
        </tr>
        <tr>
            <th>Nombre</th>
            <td>{{ entity.Nombre }}</td>
        </tr>
        <tr>
            <th>Dni</th>
            <td>{{ entity.Dni }}</td>
        </tr>
        <tr>
            <th>Especialidad</th>
            <td>{{ entity.Especialidad }}</td>
        </tr>
        <tr>
            <th>Fecha Nacimiento</th>
            <td>{{ entity.FechaNacimiento|date('Y-m-d H:i:s') }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('doctor') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('doctor_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('doctor_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
